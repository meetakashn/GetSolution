class Solution {
    public int[] getConcatenation(int[] nums) {
         int length =nums.length * 2;

        int[] ans = new int[length];
        int pos = 0;
        for (int element : nums) {
            ans[pos] = element;
            pos++;
        }

        for (int element : nums) {
            ans[pos] = element;
            pos++;
        }
       return ans;
    }
}
